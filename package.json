{
  "name": "metricalpsalmtunes",
  "version": "0.0.1",
  "description": "Universal React.js starter with the minimum possible code",
  "author": "Dieuwe de Boer",
  "license": "MIT",
  "main": "src/server.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir lib --plugins transform-react-jsx --presets es2015",
    "bundle": "./node_modules/browserify/bin/cmd.js lib/client.js -o public/js/bundle.js",
    "start": "yarn build && yarn bundle && node lib/server.js"
  },
  "dependencies": {
    "async": "^2.6.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.26",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.5.0"
  }
}
